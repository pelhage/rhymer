{
  "name": "rhymer",
  "version": "0.5.3",
  "author": "Patrick El-Hage",
  "license": "ISC",
  "description": "A command-line tool for querying Rhymezone",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/pelhage/rhymer.git"
  },
  "scripts": {
    "test": "./node_modules/.bin/jest ./src/",
    "test:watch": "./node_modules/.bin/jest ./src/ --watchAll"
  },
  "bin": {
    "rhymer": "bin/rhymer.js"
  },
  "keywords": [
    "rhymer",
    "rhymezone",
    "cli"
  ],
  "jest": {
    "verbose": true
  },
  "dependencies": {
    "axios": "^0.19.0",
    "chalk": "^2.4.1",
    "commander": "^2.18.0",
    "lodash.frompairs": "^4.0.1"
  },
  "devDependencies": {
    "husky": "^4.2.5",
    "jest": "^24.7.1",
    "lint-staged": "^10.2.2",
    "prettier": "1.14.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,md}": "prettier --write"
  }
}
